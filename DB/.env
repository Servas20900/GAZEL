# Variables de entorno para la base de datos GAZEL

# PostgreSQL Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=gazel_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=gazel_password

# Database URL (para ORMs como Prisma, TypeORM, etc.)
DATABASE_URL=postgresql://postgres:gazel_password@localhost:5432/gazel_db

# Timezone
TZ=America/Costa_Rica
